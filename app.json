{
  "expo": {
    "name": "SukeltajaApp",
    "slug": "sukeltaja-frontend",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.1.2",
    "orientation": "portrait",
    "icon": "./assets/logo_1_512.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.sukeltaja.sukeltajaapp",
      "versionCode": 6,
      "config": {
        "googleMaps": { "apiKey": "API_KEY"}
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sukeltaja.sukeltajaapp"
    }
  }
}
